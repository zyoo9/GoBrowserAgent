{
  "browser": {
    "headless": false,
    "user_data_dir": "./user_data",
    "default_width": 1280,
    "default_height": 800,
    "timeout": 30
  },
  "llm": {
    "provider": "openai",
    "api_key": "your-api-key-here",
    "api_url": "https://api.openai.com/v1",
    "model_name": "gpt-3.5-turbo",
    "max_tokens": 2048
  },
  "log": {
    "level": "info",
    "output_path": "./logs"
  },
  "web": {
    "port": 8080
  }
} 